{
  "name": "vinz",
  "version": "0.0.3",
  "description": "Enables secure storage of credentials right in your repo using AWS KMS.",
  "main": "dist/client.js",
  "scripts": {
    "test": "jest",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "lint": "eslint .",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "bin": {
    "vinz": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bjacobel/vinz.git"
  },
  "keywords": [
    "KMS",
    "secrets",
    "credentials",
    "encrypted",
    "Lambda"
  ],
  "author": "Brian Jacobel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bjacobel/vinz/issues"
  },
  "homepage": "https://github.com/bjacobel/vinz#readme",
  "dependencies": {
    "aws-sdk-umd": "0.0.2",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "ini": "^1.3.4",
    "prompt": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-jest": "^12.1.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.11.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.1",
    "jest-cli": "^12.1.1"
  },
  "engines": {
    "node": "4.3.2"
  },
  "jest": {
    "testRegex": "tests/.*-test.js$",
    "mocksPattern": "(?:[\\/]|^)mocks[\\/]",
    "collectCoverage": true
  }
}
